<a class="btn btn-info" href="/toy/add">Add new mobile</a>
<br><br>
<table class="table table-bordered">
   <tr>
      <th>Mobile model</th>
      <th>Mobile color</th>
      <th>Mobile brand</th>
      <th>Mobile image</th>
   </tr>
   {{#each toys }}
   <tr>
      <td>{{ model }}</td>
      <td>{{ color }}</td>
      <td>{{ manufacturer.name }}</td>
      <td>
         <img src="{{ image }}" alt="" width="100" height="120">
      </td>
   </tr>
   {{/each}}
</table>